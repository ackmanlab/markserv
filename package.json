{
  "name": "markserv",
  "version": "0.1.2",
  "description": "markserv serves Markdown files as GitHub style HTML and LiveReloads your files in the browser as you edit.",
  "keywords": [
    "markdown",
    "livereload",
    "server",
    "github"
  ],
  "scripts": {
    "test": "xo"
  },
  "bin": {
    "markserv": "server.js"
  },
  "author": "Alistair MacDonald <al@pwn.io>",
  "email": "al@pwn.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ackmanlab/markserv.git"
  },
  "dependencies": {
    "ansi": "^0.3.1",
    "bluebird": "^3.5.0",
    "commander": "^2.9.0",
    "connect": "^3.6.0",
    "connect-livereload": "^0.6.0",
    "html-pdf": "^2.1.0",
    "jsdom": "^9.12.0",
    "less": "^2.7.2",
    "livereload": "^0.6.2",
    "livereload-js": "^2.2.2",
    "markdown-it": "^8.3.1",
    "markdown-it-deflist": "^2.0.1",
    "markdown-it-footnote": "https://github.com/ackmanlab/markdown-it-footnote.git",
    "markdown-it-front-matter": "^0.1.2",
    "markdown-it-mathjax": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "mime": "^1.3.4",
    "open": "0.0.5",
    "openport": "0.0.4",
    "send": "^0.15.1",
    "websocket.io": "^0.2.1"
  },
  "devDependencies": {
    "connect-livereload": "^0.6.0",
    "livereload": "^0.6.2",
    "xo": "^0.18.0"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "globals": [
      "it",
      "describe",
      "beforeEach"
    ],
    "rules": {
      "no-use-extend-native": 0
    }
  }
}
